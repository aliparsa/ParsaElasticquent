{
  "name": "aliparsa/parsaelasticquent",
  "type": "library",
  "description": "Maps Laravel Eloquent models to Elasticsearch types.",
  "keywords": [
    "elasticsearch",
    "eloquent",
    "laravel"
  ],
  "homepage": "https://github.com/aliparsa/Elasticquent",
  "license": "MIT",
  "authors": [
    {
      "name": "Ali Parsa",
      "email": "ali.parsa.p@gmail.com",
      "homepage": "https://github.com/aliparsa/Elasticquent"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "illuminate/database": "~4.2|^5|^6|^7|^8",
    "illuminate/config": "~4.2|^5|^6|^7|^8",
    "nesbot/carbon": "~1.0|~2",
    "elasticsearch/elasticsearch": "v7.11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.2|~5.0|~8.0",
    "mockery/mockery": "^0.9.4|^1.0"
  },
  "autoload": {
    "psr-4": {
      "Elasticquent\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
